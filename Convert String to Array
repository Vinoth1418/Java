import java.util.*;

public class Main {
    public static void main(String[] args) {
      String a="Computer science";
      char b[]=a. toCharArray();
     // System. out. print("[");
      for(int i=0;i<b.length;i++){
        System.out.println(b[i]+" ");
      }
      //System. out. println("]");
  }
}
