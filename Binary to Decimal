import java.util.*;
class Main{
    public static void main(String args[]){
       Scanner a=new Scanner(System.in);
        System.out.println("Enter a number:");
       int b=a.nextInt();
       int sum=0;
       int x=1;
       while(b>0){
           int c=b%10;
           sum+=(b%10)*x;
           b/=10;
           x*=2;
       }
        System.out.println(sum);
    }
}
