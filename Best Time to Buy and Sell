class Main {
    public static void main(String[] args) {
           int a[]={7,1,5,3,6,4};
           int min=Integer.MAX_VALUE;
           int profit=0;
           for(int i=0;i<a.length;i++){
               if(a[i]<min){
                   min=a[i];
               }
               else{
                   int max=a[i]-min;
                   if(max>profit){
                       profit=max;
                   }
               }
           }
           System.out.println(profit);
    }
}
