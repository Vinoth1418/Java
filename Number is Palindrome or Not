import java.util.*;

public class Main {
    public static void main(String[] args) {
      int num=126;
      int a=num;
      int rev=0;
      while(num>0){
        rev=rev*10+num%10;
        num=num/10;
      }
      if(rev==a)
      System. out. println(a+" is a Palindrome");
      else
      System. out. println(a+" is not a Palindrome");
      }}
