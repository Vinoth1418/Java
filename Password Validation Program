import java.util.*;
class main {
    public static void check(String a) {
        int points=0;
        if(a.length()<=8){
            points+=18;
        }
        if(a.length()>8){
            points+=20;
        }
          if(a.matches(".*[A-Z].*")){
              points+=20;
          }
         if(a.matches(".*[!@#$%^&*()].*")){
             points+=20;
         }
         if(a.matches(".*[a-z].*")){
              points+=20;
        }
         if(a.matches(".*[0-9]")){
             points+=20;
         }
        System.out.println("Points:"+points);
         if(points<=50){
             System.out.println("Week password");
         }
         else if(points<=70){
             System.out.println("Not bad");
         }
         else{
             System.out.println("Strong password");
         }
        }

    public static void main(String[] args) {
        String a="Vinoth@123";
        check(a);

    }
}
