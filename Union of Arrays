import java.util.*;

public class Main {
    public static void main(String[] args) {
      int a[]={1,2,3,4,5};
      int b[]={1,2,3};
      int count=0;
      HashSet<Integer>c=new HashSet<>();
      for(int i=0;i<a.length;i++){
        c.add(a[i]);
      }
      for(int i=0;i<b.length;i++){
        c.add(b[i]);
        }
        for(int i=0;i<c.size();i++){
        count++;
      }
      System.out.println(count);
    }
  }
