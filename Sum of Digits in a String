import java.util.*;
class Main {
   public static int sum(String a){
       int sum=0;
       char b[]=a.toCharArray();
       for(int i=0;i<a.length();i++ ){
           if(b[i]>='0'&&b[i]<='9'){
               sum+=(b[i]-'0');
           }
       }
       return sum;
        }

    public static void main(String[] args) {
        String a="vin57h3n8";
        int res=sum(a);
        System.out.println(res);
    }
    }
