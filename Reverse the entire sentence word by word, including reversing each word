import java.util.*;
class Main {
    public static void lon(String a) {
        String b="";
        String d[]=a.split(" ");
        for(int i=d.length-1;i>=0;i--){
            String c=d[i];
            for(int j=c.length()-1;j>=0;j--){
                b+=c.charAt(j);
            }
            b+=" ";

        }
        System.out.println(b);
        }



    public static void main(String[] args) {
       String a="I love coding";
       lon(a);
    }
}


