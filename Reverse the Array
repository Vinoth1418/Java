import java.util.*;

public class Main {
    public static void main(String[] args) {
      int a[]={7,14,21,28,35};
      System. out. print("Before reversing:");
      for(int i=0;i<a.length;i++){
        System. out. print(a[i]+" ");
      }
      System. out.println();
      System. out. print("After reversing:");
      int len=a.length;
      for(int i=len-1;i>=0;i--){
        System.out.print(a[i]+" ");
      }
}}
