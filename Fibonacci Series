import java.util.*;

public class Main {
    public static void main(String[] args) {
      int n1=0,n2=1,n3;
      int a=5;
      System.out.print("Fibonacci Series:"+n1+" ");
      for(int i=0;i<a;i++){
        n3=n2+n1;
        n1=n2;
        n2=n3;
      
        System.out.print(n2+" ");
      }  
        
      
      
  }
}
