import java.util.*;

public class Main {
    public static void main(String[] args) {
      int a=7;
      for(int i=1;i<=a;i++){
        for(int j=i;j<a;j++){
          System.out.print(" ");}
           for(int k=0;k<a;k++){
             System.out.print("*");
           }
      
      System.out.println();
      }
      }}
