import java.util.*;

public class Main {
    public static void main(String[] args) {
      String a="abcde";
      char b[]=a.toCharArray();
      for(int i=0;i<a.length();i++){
        for(int j=0;j<=i;j++){
          System.out.print(b[i]);
        }
        System. out. println();
      }
  }
}
