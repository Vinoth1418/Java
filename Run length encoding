import java.util.*;

public class Main {
    public static void main(String[] args) {
      String a="wwwwaaadexxxxxxywww";
      int b=a.length();
      for(int i=0;i<b;i++){
        int count=1;
        while(i<b-1&&a.charAt(i)==a.charAt(i+1)){
          count++;
          i++;
        }
        System.out.print(a.charAt(i)+""+count);
      }
  }
}
