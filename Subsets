import java.util.*;

public class Main {
    public static void main(String[] args) {
      String a="abc";
      int len=a.length();
      int temp=0;
      String b[]=new String[len*(len+1)/2];
      for(int i=0;i<len;i++){
        for(int j=i;j<len;j++){
          b[temp]=a.substring(i,j+1);
          temp++;
        }}
      System. out. print("[");
      for(int i=0;i<b.length;i++){
       System. out. print("["+b[i]+"]"+"");
  }
  System. out. print("]");
}}
