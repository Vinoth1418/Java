import java.util.*;

public class Main {
    public static void main(String[] args) {
      int a[]={1, 2, 3, 1, 4, 5, 2, 3, 6};
      int k=3;
      for(int i=0;i<=a.length-k;i++){
        int max=a[0];
        for(int j=0;j<k;j++){
          if(a[i+j]>max){
          max=a[i+j];
        }}
        System.out.print(max+" ");
      }
  }
}
