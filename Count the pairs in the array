import java.util.*;

public class Main {
    public static void main(String[] args) {
      int a[]={1, 5, 7, -1, 5};
      int target=6;
      int count=0;
      for(int i=0;i<a.length;i++){
        for(int j=i+1;j<a.length;j++){
          if(a[i]+a[j]==target){
            count++;
          }
        }
  }
  System.out.println(count);
}
}
