import java.util.*;

public class Main {
    public static void main(String[] args) {
      int a[]={2,4,6,8,9,10,12};
      int b[]={2,4,6,8,10,12};
     for(int i=0;i<a.length;i++){
       int found=0;
       for(int j=0;j<b.length;j++){
         if(a[i]==b[j]){
         found=1;
         }}
         if(found==0){
           System.out.println(a[i]+" is the extra element in array 1 at index "+i);
         
       }
     }
  }
}
