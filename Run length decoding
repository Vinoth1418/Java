import java.util.*;

public class Main {
    public static void main(String[] args) {
      String a="v4i4n4o4";
      StringBuilder b=new StringBuilder();
      for(int i=0;i<a.length()-1;i=i+2){
        char c=a.charAt(i);
        int count=a.charAt(i+1)-'0';
        for(int j=0;j<count;j++){
          b.append(c);
        }
      }
      System.out.println(b.toString());
  }
}
