import java.util.*;

public class Main {
    public static void main(String[] args) {
      int a=31;
      boolean b=true;
      for(int i=2;i<=Math.sqrt(a);i++){
        if(a%i==0){
          b=false;
          break;
        }
      }
        if(b){
          System.out.println("True");
        }
        else{
          System.out.println("False");
        }
  }
}
