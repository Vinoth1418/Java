import java.util.*;

public class Main {
    public static void main(String[] args) {
      int a[]={4,7,8,4,3,7,2,1};
       System. out. println("Duplicates elements in the given array:");
       for(int i=0;i<a.length;i++){
        for(int j=i+1;j<a.length;j++){
          if(a[i]==a[j]){
          System. out. println(a[i]);
          }
          }
      }
    }
}
