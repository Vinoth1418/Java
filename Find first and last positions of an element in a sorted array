import java.util.*;

public class Main {
    public static void main(String[] args){
      int a[]={1, 3, 5, 5, 5, 5, 67, 123, 125};
      int k=5;
       int first=-1;
      for(int i=0;i<a.length;i++){
        if(a[i]==k){
          first=i;
          break;
        }}
        int last=-1;
        for(int j=a.length-1;j>0;j--){
          if(a[j]==k){
            last=j;
          break;
        }
      }
      int res[]={first,last};
      System.out.print(Arrays.toString(res));
    }
  }
