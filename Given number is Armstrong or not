import java.util.*;

public class Main {
    public static void main(String[] args) {
      int a=407;
      int temp=a;
      int r;
      int sum=0;
      while(a>0){
        r=a%10;
        a=a/10;
        sum=sum+r*r*r;
      }
      if(sum==temp){
      System. out. println(temp+" is an Armstrong number");
}
      else{
        System. out. println(temp+" is not an Armstrong number");
        }
    }}
