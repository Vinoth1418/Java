import java.util.*;

public class Main {
    public static void main(String[] args) {
      int a[]={3,6,0,2,0,4,0,5};
      int b=0;
      for(int i=0;i<a.length;i++){
      if(a[i]!=0){
        a[b]=a[i];
        System.out.print(a[b]+" ");
        b++;
      }}
      for(int j=b;j<a.length;j++){
        a[j]=0;
        System.out.print(a[j]+" ");
      
      }
      
  }
}
