import java.util.*;

public class Main {
    public static void main(String[] args) {
      int prices[]={1,12,5,111,200,1000,10};
      int money=50;
      int sum=0;
      int count=0;
      Arrays.sort(prices);
       for(int i=0;i<prices.length;i++){
         if(sum+prices[i]<=money){
           sum=sum+prices[i];
           count++;
         }
       }
          
        
        System.out.println("Maximun number of toys can be purchased:"+count);
      
  }
}
