import java.util.*;

public class Main {
    public static void main(String[] args) {
      String a="dcba";
      char b[]=a. toCharArray();
      char temp=' ';
      for(int i=0;i<a.length();i++){
        for(int j=i+1;j<a.length();j++){
          if(b[i]>b[j]){
           temp=b[i];
          b[i]=b[j];
          b[j]=temp;
        }}
      }
        System. out. println(b);
      
  }
}
