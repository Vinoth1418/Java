public class Solution {
    public static void main(String args[]) {
        int arr[]={1,2,3,5};
        int sum = 0;
        int res;
        int n = arr.length + 1;
        int expectedSum = (n * (n + 1)) / 2;
        for (int i = 0; i < arr.length; i++) {
            sum += arr[i];
        }
        res=expectedSum-sum;
        System.out.println("Missing element is:"+res);
        
    }
}
