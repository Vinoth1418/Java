import java.util.*;

public class Main {
    public static void main(String[] args) {
      int a[]={2,5,6,7,8,7,9,4,7};
      int target=7;
      int count=0;
      for(int i=0;i<a.length;i++){
        if(a[i]==target){
          count++;
        }
          }
      System. out. println("Number of occurances of "+target+" is:"+count);
      
  }
}
