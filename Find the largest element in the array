import java.util.*;

public class Main {
    public static void main(String[] args) {
     int a[]={7,21,14,35,21};
     int max=a[0];
     for(int i=1;i<a.length;i++){
       if(a[i]>max){
         max=a[i];
       }
       }
       System. out. println("Largest element in the array is:"+max);
     }
  }
