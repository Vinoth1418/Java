import java.util.*;

public class Main {
    public static void main(String[] args) {
     int a[]={12,345,2,6,7896};
     int count=0;
    for(int num:a){
      int digits=0;
      int temp=num;
      while(temp>0){
        temp/=10;
        digits++;
      }
      if(digits%2==0){
        count++;
      }
    }
      System.out.println(count); 
  }
}
