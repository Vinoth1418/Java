import java.util.*;

public class Main {
    public static void main(String[] args) {
      String a="Engineering";
      String rev="";
      for(int i=a.length()-1;i>=0;i--){
       rev=rev+a.charAt(i);
      }
      System. out. println("Original String:"+a);
      System. out. println("Reversed String is:"+rev);
}}
