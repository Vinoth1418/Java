import java.util.*;

public class Main {
    public static void main(String[] args) {
      int a[]={1,6,9,4,3,7,8};
      Arrays.sort(a);
      int n=a.length;
      int i=0;
      int j=a.length-1;
      while(i<j){
        System.out.print(a[j--]+" ");
        System.out.print(a[i++]+" ");}
        if(n%2!=0)
        System.out.print(a[i]);
      
  }
}
