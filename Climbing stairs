import java.util.*;

public class Main {
    public static void main(String[] args) {
      int a=6;
      if(a==0){
        System.out.println(a);
      }
      if(a==1){
        System.out.println(a);
      }
      int b[]=new int[a+1];
      b[0]=1;
      b[1]=1;
      for(int i=2;i<=a;i++){
        b[i]=b[i-1]+b[i-2];
      }
      System.out.println(b[a]);
  }
}
