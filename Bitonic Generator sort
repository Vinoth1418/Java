import java.util.*;

public class Main {
    public static void main(String[] args) {
        int a[]={0,1,2,3,4,5,6,7};
        for(int i=0;i<a.length;i++){
          if(i%2==0)
          System.out.print(a[i]+" ");
        }
        for(int j=a.length-1;j>0;j--){
          if(j%2==1)
          System.out.print(a[j]+" ");
        }
  }
}
