import java.util.*;

public class Main {
    public static void main(String[] args) {
      int num[][]=new int[3][4];
      int value=1;
      for(int i=0;i<3;i++){
        for(int j=0;j<4;j++){
          num[i][j]=value;
          value++;
          System.out.print(num[i][j]+" ");
          
        }
        System. out. println();
      }}
}
