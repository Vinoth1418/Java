import java.util.*;

public class Main {
    public static void main(String[] args) {
      int a[]={1,2,3,7,5};
      int sum=12;
      ArrayList<Integer>b=new ArrayList<>();
      for(int i=0;i<a.length;i++){
        for(int j=i+1;j<a.length;j++){
          if(a[i]+a[j]==sum){
          b.add(i);
          b.add(j);
        }}}
          System.out.println(b);
      
          }
        }
      
  
